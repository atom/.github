$(document).scroll(function () {
var el = $('div.page-navigation')
var top = $('div#page-nav-fixer').offset().top - $(document).scrollTop();
// var con = $('div#page-nav-fixer')
// var scrollled = $(window).scrollTop();

if (top < 60 && !el.is('.fixHeader')) {
$(el).addClass('fixHeader');
}
if (top > 60 && el.is('.fixHeader')) {
$(el).removeClass('fixHeader');
}
});
