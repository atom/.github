let symcDataLayer = (function() {

let languageMap = new Map([
["nl", "nl"],
["de", "de"],
["fr", "fr"],
["uk", "uk"],
["us", "en"]
]);

let subSections = {
homepage: "homepage",
products: "products",
legal: "legal",
support: "support",
news: "news",
about: "about",
blog: "blog",
company: "company",
searchResults: "search-results"
};

let pages = {
"/repository/": {
subSection: subSections.legal,
pageName:"repository",
},
};

// Adobe Analytics Variables. Initial assignments act as the default values.
let countryCode = "us";
let languageCode = "en";
let section = "quovadisglobal";
let subSection = "na";
let subSubSection = "na";
let subSubSubSection = "na";
let pageName = "na";

// URL path variables.
let locationPath = window.location.pathname;
let locationPathArray = locationPath.split("/").filter(value => value != ""); // Filtering empty strings.

let hasCountryCode = false;
if (locationPathArray.length > 0) {
for (let countryCode of languageMap.keys()) {
if (locationPathArray[0] == countryCode) {
hasCountryCode = true;
break;
}
}

if (hasCountryCode) {
countryCode = locationPathArray.shift();
languageCode = languageMap.get(countryCode);
locationPath = locationPath.slice(countryCode.length + 1); // Removing the country code from the location path.
}
}

if (locationPath in pages) {
let pageData = pages[locationPath];
subSection = pageData.subSection;
pageName = pageData.pageName;
} else {
// otherwise default to...
subSection = locationPathArray.shift();
pageName = locationPathArray.shift();
}

return {
om: {
//"account": "veritassymantecwebsitesecuritydev", // dev
account: "veritassymantecwebsitesecurity", // Prod

content_format: "html",
content_type: "page",
content_title: document.title,

site_country: countryCode,
site_language: languageCode,
site_section: section,
site_sub_section: subSection,
site_sub_sub_section: subSubSection,
site_sub_sub_sub_section: subSubSubSection,
page_name: pageName
}
};
})();

